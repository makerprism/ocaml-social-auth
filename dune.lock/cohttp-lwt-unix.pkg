(version 6.1.1)

(build
 (progn
  (when
   %{pkg-self:dev}
   (run dune subst))
  (run dune build -p %{pkg-self:name} -j %{jobs} @install)))

(depends
 dune
 ocaml
 http
 cohttp
 cohttp-lwt
 cmdliner
 lwt
 conduit-lwt
 conduit-lwt-unix
 fmt
 base-unix
 ppx_sexp_conv
 magic-mime
 logs)

(source
 (fetch
  (url
   https://github.com/mirage/ocaml-cohttp/releases/download/v6.1.1/cohttp-6.1.1.tbz)
  (checksum
   sha256=6b420c56203b3a0b515964f036bcea0fb9a362876b5791cd7ff50e12366c489c)))
